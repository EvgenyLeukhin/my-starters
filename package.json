{
  "name": "webpack-mpa-starter",
  "version": "1.0.0",
  "description": "Build MPA with Webpack",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --open google-chrome",
    "build": "webpack --config=webpack.config.prod.js",
    "serve": "node server.js --open google-chrome",
    "build-and-serve": "npm run build && npm run serve",
    "delete-build": "rimraf public || echo 'Alredy deleted'",
    "install-packages-npm": "npm install",
    "install-packages-yarn": "yarn install",
    "delete-packages": "rimraf node_modules || echo 'Alredy deleted'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvgenyLeukhin/my-starters.git"
  },
  "keywords": [
    "webpack-babel",
    "webpack-babel-starter",
    "webpack-map",
    "webpack-mpa-starter"
  ],
  "author": "EvgenyLeukhin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvgenyLeukhin/my-starters/issues"
  },
  "homepage": "https://github.com/EvgenyLeukhin/my-starters#readme",

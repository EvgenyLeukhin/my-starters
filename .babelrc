{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "> 0.5%", "not ie <= 10"]
      },
      "debug": true
    }]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "ignore": ["/node_modules/"]
}
